{
  "name": "kotori",
  "description": "Kotori is a tool that automatically format and evaluate for CSS",
  "version": "0.8.1",
  "author": "kubosho",
  "bin": {
    "kotori": "bin/kotori.js"
  },
  "bugs": {
    "url": "https://github.com/kubosho/kotori/issues"
  },
  "dependencies": {
    "autoprefixer": "^6.1.0",
    "chokidar": "^1.2.0",
    "clean-css": "^3.4.8",
    "cssfmt": "^1.3.8",
    "moment": "^2.10.6",
    "object-assign": "^4.0.1",
    "optionator": "^0.6.0",
    "postcss": "^5.0.12",
    "postcss-reporter": "^1.3.0",
    "stylelint": "^2.3.1",
    "stylelint-config-suitcss": "^0.5.1",
    "stylestats": "^6.0.0",
    "through2": "^2.0.0",
    "user-home": "^2.0.0",
    "vinyl-fs": "^2.2.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "coveralls": "^2.11.4",
    "empty-trash-cli": "^1.0.0",
    "esdoc": "^0.4.3",
    "eslint": "^1.9.0",
    "espower-babel": "^3.3.0",
    "fs-extra": "^0.26.2",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.4",
    "npm-check": "^4.1.2",
    "power-assert": "^1.2.0",
    "trash-cli": "^1.2.0",
    "vinyl": "^1.0.0"
  },
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "files": [
    "bin",
    "conf",
    "lib"
  ],
  "homepage": "https://github.com/kubosho/kotori#readme",
  "keywords": [
    "css",
    "development"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kubosho/kotori.git"
  },
  "scripts": {
    "build": "npm run trash && babel src --out-dir lib & esdoc -c esdoc.json",
    "ci": "npm run build && npm run coverage && npm run trash && empty-trash",
    "coverage": "istanbul cover _mocha -- --compilers js:babel/register",
    "lint": "eslint src/**/*.js",
    "package-update": "npm-check -u",
    "start": "npm run build && npm test",
    "trash": "trash build stats lib",
    "test": "mocha --compilers js:espower-babel/guess --require test/bootstrap.js test/**/*.test.js",
    "watch": "babel src --watch --out-dir lib"
  }
}
