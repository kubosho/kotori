{
  "name": "nozomi",
  "description": "\"Nozomi\" helps you write awesome CSS",
  "version": "1.0.0",
  "author": "kubosho",
  "bin": {
    "nozomi": "bin/nozomi.js"
  },
  "bugs": {
    "url": "https://github.com/kubosho/nozomi/issues"
  },
  "dependencies": {
    "autoprefixer": "^6.0.3",
    "clean-css": "^3.4.4",
    "csscomb": "^3.1.8",
    "cssfmt": "^1.3.4",
    "object-assign": "^4.0.1",
    "optionator": "^0.6.0",
    "postcss": "^5.0.6",
    "postcss-reporter": "^1.2.1",
    "stylelint": "^1.1.0",
    "stylelint-config-suitcss": "^0.5.0",
    "stylestats": "^5.4.3",
    "through2": "^2.0.0",
    "user-home": "^2.0.0",
    "vinyl-fs": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "coveralls": "^2.11.4",
    "del": "^2.0.2",
    "esdoc": "^0.3.0",
    "eslint": "^1.5.1",
    "espower-babel": "^3.3.0",
    "fs-extra": "^0.24.0",
    "istanbul": "^0.3.21",
    "mocha": "^2.3.3",
    "npm-check": "^4.0.3",
    "power-assert": "^1.0.1",
    "vinyl": "^0.5.3"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "bin",
    "lib"
  ],
  "homepage": "https://github.com/kubosho/nozomi#readme",
  "keywords": [
    "css",
    "development"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kubosho/nozomi.git"
  },
  "scripts": {
    "build": "babel src --out-dir lib & esdoc -c esdoc.json",
    "coverage": "istanbul cover _mocha -- --compilers js:babel/register",
    "lint": "eslint src/**/*.js",
    "start": "npm run build && npm test",
    "test": "mocha --compilers js:espower-babel/guess test/**/*.test.js",
    "watch": "babel src --watch --out-dir lib"
  }
}
